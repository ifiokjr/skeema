{
  "extends": ["@kj/tslint-rules/base"],
  "rules": {
    "no-implicit-dependencies": [
      true,
      "dev",
      [
        "jest-extended",
        "firebase-tools",
        "jest-cli",
        "chalk",
        "ps-tree",
        "lodash",
        "debug",
        "execa",
        "wait-on",
        "listr",
        "inquirer"
      ]
    ],
    "no-any": false,
    "max-classes-per-file": false,
    "no-empty-interface": false,
    "no-console": false
  }
}
